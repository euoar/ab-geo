{
  "name": "ab-geo",
  "version": "0.0.11",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "prerelease": "standard-version",
    "release": "git push --follow-tags origin master",
    "predeploy": "npm run release",
    "deploy": " ng deploy --base-href=/ab-geo/",
    "build:prod:stats": "ng build --prod --stats-json",
    "analyze": "npm run build:prod:stats && webpack-bundle-analyzer ./dist/ab-geo/stats-es2015.json",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "sample-pre-commit": "pretty-quick --staged & ng lint --fix",
      "pre-push": ""
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "protractor": "~7.0.0",
    "standard-version": "^9.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
